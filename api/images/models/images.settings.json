{
  "kind": "collectionType",
  "collectionName": "images",
  "info": {
    "name": "Images"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "imageFile": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true
    },
    "imageLocation": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "imageURL": {
      "type": "string",
      "required": true,
      "unique": false
    }
  }
}
